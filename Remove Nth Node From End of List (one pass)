/**
 * Definition for singly-linked list.
 * function ListNode(val, next) {
 *     this.val = (val===undefined ? 0 : val)
 *     this.next = (next===undefined ? null : next)
 * }
 */
/**
 * @param {ListNode} head
 * @param {number} n
 * @return {ListNode}
 */
var removeNthFromEnd = function(head, n) {
    
    //add sentinel node
    let sentinel=new ListNode();
    sentinel.next=head;
    
    //move first to n position
    let first=sentinel;
    for(let i=0;i<n;i++){
        first=first.next;
    }
    //run loop till first node reaches last
    let second=sentinel;
    while(first.next){
        second=second.next;
        first=first.next;
    }
        //delete second.next

    second.next=second.next.next;
    return sentinel.next;
};
